{
    "name": "@stacks/react-app",
    "version": "1.0.0",
    "description": "Stacks react app",
    "private": true,
    "homepage": "./",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --silent --watchAll=false",
        "test:update": "react-scripts test --watchAll=false -u",
        "test:cover": "react-scripts test --silent --watchAll=false --coverage",
        "eject": "react-scripts eject"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "/tests/",
            "index.ts",
            "typings.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        }
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.11.4",
        "@testing-library/react": "11.1.0",
        "@testing-library/react-hooks": "7.0.1",
        "@testing-library/user-event": "12.1.10",
        "@types/electron": "1.6.10",
        "@types/enzyme": "3.10.9",
        "@types/jest": "26.0.24",
        "@types/lodash": "4.14.172",
        "@types/node": "12.0.0",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-redux": "7.1.18",
        "@types/react-router": "^5.1.16",
        "@types/react-router-dom": "^5.1.8",
        "@types/redux-mock-store": "1.0.3",
        "@types/styled-components": "5.1.12",
        "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
        "enzyme": "3.11.0",
        "enzyme-to-json": "3.6.2",
        "redux-mock-store": "1.5.4"
    },
    "dependencies": {
        "axios": "0.21.1",
        "bootstrap": "5.0.1",
        "loaders.css": "0.1.2",
        "lodash": "4.17.21",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-bootstrap": "1.6.0",
        "react-dom": "17.0.2",
        "react-redux": "7.2.4",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "react-spinner-material": "1.3.1",
        "redux": "4.1.0",
        "redux-thunk": "2.3.0",
        "styled-components": "5.3.0",
        "typescript": "4.1.2",
        "web-vitals": "1.0.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
